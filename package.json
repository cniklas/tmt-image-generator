    "@unocss/reset": "^0.63.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript": "~5.6.2",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.6"
  }
